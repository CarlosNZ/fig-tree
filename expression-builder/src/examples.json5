[
  // Compare a PLUS operator to a shorthand version of the same thing
  {
    operator: '+',
    values: [
      {
        $plus: [2, 2],
      },
      {
        operator: '+',
        values: [2, 2],
      },
    ],
  },

  // A simple shorthand node (with string property)
  {
    $getData: 'user.firstName',
  },

  // Same as above but twice, one shorthand one not.
  {
    operator: '+',
    values: [
      {
        $getData: 'user.firstName',
      },
      ' ',
      {
        operator: 'getData',
        property: 'user.lastName',
      },
      {
        operator: '+',
        values: [2, 2],
      },
    ],
  },
]
