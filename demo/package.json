{
  "name": "expression-evaluate-gui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.1.2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "expression-evaluator": "file:..",
    "framer-motion": "^6.3.6",
    "loose-json": "^1.1.2",
    "pg": "^8.7.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "rimraf ./src/expression-evaluator && mkdir ./src/expression-evaluator && concurrently --kill-others-on-fail \"yarn serve\" \"PORT=3005 react-scripts start\" \"nodemon watch.js\"",
    "serve": "node ./src/express/server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "../src"
    ],
    "ext": "js, json, ts, txt",
    "delay": 200
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.2",
    "@types/node": "^17.0.38",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "concurrently": "^7.2.1"
  }
}
