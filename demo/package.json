{
  "name": "fig-tree-demo",
  "version": "0.9.0",
  "private": true,
  "homepage": "https://carlosnz.github.io/fig-tree-evaluator",
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fig-tree-evaluator": "^2.8.5",
    "framer-motion": "^10.2.5",
    "loose-json": "^1.1.2",
    "pg": "^8.10.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "web-vitals": "^3.3.0"
  },
  "scripts": {
    "start": "rimraf ./src/fig-tree-evaluator && mkdir ./src/fig-tree-evaluator && mkdir ./src/fig-tree-evaluator/src && concurrently --kill-others-on-fail \"yarn serve\" \"PORT=3005 react-scripts start\" \"nodemon watch.js\"",
    "serve": "node ./src/express/server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "../src"
    ],
    "ext": "js, json, ts, txt",
    "delay": 200
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^7.2.1",
    "gh-pages": "^5.0.0",
    "nodemon": "^2.0.21",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  }
}
