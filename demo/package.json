{
  "name": "fig-tree-demo",
  "version": "0.9.0",
  "private": true,
  "homepage": "https://carlosnz.github.io/fig-tree-evaluator",
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fig-tree-evaluator": "^2.11.7",
    "framer-motion": "^10.15.1",
    "loose-json": "^1.1.2",
    "pg": "^8.11.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "web-vitals": "^3.4.0"
  },
  "scripts": {
    "start": "rimraf ./src/fig-tree-evaluator && rimraf ./src/expression-builder && mkdir ./src/fig-tree-evaluator && mkdir ./src/fig-tree-evaluator/src && mkdir ./src/expression-builder && mkdir ./src/expression-builder/src && concurrently --kill-others-on-fail \"yarn serve\" \"PORT=3005 react-scripts start\" \"nodemon watch.js\"",
    "serve": "node ./src/express/server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "../src",
      "../expression-builder/src"
    ],
    "ext": "js, json, ts, tsx, txt",
    "delay": 200
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.0",
    "gh-pages": "^5.0.0",
    "nodemon": "^3.0.1",
    "object-property-assigner": "^1.0.1",
    "react-scripts": "5.0.1",
    "typescript": "^5.1.6"
  }
}
